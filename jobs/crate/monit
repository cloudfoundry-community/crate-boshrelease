check process crate
  with pidfile /var/vcap/sys/run/crate/crate.pid
  start program "/var/vcap/packages/bosh-helpers/monit_debugger crate_ctl '/var/vcap/jobs/crate/bin/crate_ctl start'"
  stop program "/var/vcap/packages/bosh-helpers/monit_debugger crate_ctl '/var/vcap/jobs/crate/bin/crate_ctl stop'"
  group vcap
